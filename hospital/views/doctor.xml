<odoo>
    <data>
<!--Vista de lista-->
        <record model="ir.ui.view" id="hospital.doctor_list">
        <field name="name">hospital doctor list</field>
        <field name="model">hospital.doctor</field>
        <field name="arch" type="xml">
            <list>
            <field name="name"/>
            <field name="surname"/>
            <field name="phone"/>
            <field name="specialty"/>
            <field name="registration_number"/>
            <field name="admision_ids"/>

            </list>
        </field>
        </record>


        <!--Vista de formulario-->
        <record model="ir.ui.view" id="hospital.doctor_form">
        <field name="name">hospital doctor form</field>
        <field name="model">hospital.doctor</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name"/>
                    <field name="surname"/>
                    <field name="phone"/>
                    <field name="specialty"/>
                    <field name="registration_number"/>
                    <field name="admision_ids"/>

                </group>
            </form>
        </field>
        </record>
    </data>
</odoo>